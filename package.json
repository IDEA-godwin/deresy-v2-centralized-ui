{
  "name": "deresy-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development",
    "build:staging": "vue-cli-service build --mode staging",
    "deploy:dev": "yarn build:dev && firebase deploy -P development --only hosting",
    "deploy:staging": "yarn build:staging && firebase deploy -P staging --only hosting",
    "deploy:prod": "yarn build && firebase deploy -P production",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "uglify": "./node_modules/.bin/uglifyjs $1 --compress --output ./dist/uglyfy/$1"
  },
  "dependencies": {
    "@babel/core": "^7.24.3",
    "@babel/eslint-parser": "^7.24.1",
    "@element-plus/icons": "^0.0.11",
    "@ethereum-attestation-service/eas-sdk": "^2.7.0",
    "@kyvg/vue3-notification": "^2.3.4",
    "@reown/appkit": "^1.6.0",
    "@reown/appkit-adapter-wagmi": "^1.6.2",
    "@tanstack/vue-query": "^5.62.8",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vuelidate/core": "^2.0.0-alpha.42",
    "@vuelidate/validators": "^2.0.0-alpha.30",
    "@wagmi/vue": "^0.1.3",
    "axios": "^0.21.4",
    "configcat-js": "5.7.1",
    "core-js": "^3.6.5",
    "crypto-browserify": "^3.12.0",
    "element-plus": "2.2.8",
    "eslint": "^8.57.0",
    "ethers": "^6.13.5",
    "firebase": "^8.8.0",
    "marked": "4.3.0",
    "quill-delta-to-html": "^0.12.1",
    "react": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "urql": "^4.2.1",
    "viem": "^2.21.55",
    "vm-browserify": "^1.1.2",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.1.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@types/p5": "^1.3.1",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^9.1.1",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "prettier": "^2.2.1",
    "sass": "^1.63.6",
    "sass-loader": "^10",
    "typescript": "~3.9.3",
    "uglify-js": "^3.14.3",
    "vue-jest": "^5.0.0-0",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "git add"
    ]
  }
}
